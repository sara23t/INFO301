                                <tr>
                                    <td>
                                        <%= userData[1][i].first_name %>
                                    </td>
                                    <td>
                                        <%= userData[1][i].surname %>
                                    </td>
                                    <td>
                                        <%=userData[1][i].mentor_username %>
                                    </td>
                                    <td>
                                        <%= userData[1][i].email_address %>
                                    </td>
                                    <td>
                                        <%= userData[1][i].phone_number %>
                                    </td>
                                    <td>
                                        <% for(var j=0;j<userData[2].length;j++){ %>
